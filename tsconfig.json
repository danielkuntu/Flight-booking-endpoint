{
  "compilerOptions": {
    "target": "ES2020",
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "lib": ["es2015", "es6", "dom", "dom.iterable"]
}